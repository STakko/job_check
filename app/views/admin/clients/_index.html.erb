<table class="table">
  <thead class="thead-dark">
    <tr>
      <th>会社名</th>
      <th>電話番号</th>
      <th>担当者名</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% clients.each do |client| %>
    <tr>
      <td>
        <%= link_to admin_client_path(client) do %>
          <%= client.name %>
        <% end %>
      </td>
      <td>
        <%= client.phone_number %>
      </td>
      <td>
        <%= client.supervisor %>
      </td>
      <td>
        <%= link_to '編集', edit_admin_client_path(client.id), class: "btn btn-warning" %>
      </td>
    </tr>
    <% end %>
    <%= paginate clients %>
  </tbody>
</table>